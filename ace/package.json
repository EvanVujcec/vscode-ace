{
    "name": "ace",
    "displayName": "Ace",
    "description": "Ace language https://github.com/yosssi/ace support for Visual Studio Code based off https://github.com/yosssi/ace-tmbundle",
    "version": "0.0.1",
    "publisher": "Evan Vujcec",
    "icon": "images/ace.png",
    "engines": {
        "vscode": "^0.10.10"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "ace",
                "aliases": [
                    "ace-lang",
                    "ace"
                ],
                "extensions": [
                    ".ace"
                ],
                "configuration": "./ace.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "ace",
                "scopeName": "text.ace",
                "path": "./syntaxes/ace.tmLanguage"
            }
        ]
    }
}